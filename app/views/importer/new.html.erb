<h1>Import your Products Here</h1>
<%= content_tag(:div, flash[:notice]) if flash[:notice] %>

<%= form_tag importer_path, :multipart => true do %>
  <p>
    <%= label_tag "add your products list" %>
    <br />
    <%= file_field_tag "importer[products_list]" %>
  </p>
  <p>
    <%= submit_tag "upload" %>
<% end %>

